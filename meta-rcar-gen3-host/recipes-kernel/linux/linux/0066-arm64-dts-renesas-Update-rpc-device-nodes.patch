+			compatible = "renesas,r8a77960-rpc-if", "renesas,rcar-gen3-rpc-if";
 			reg = <0 0xee200000 0 0x1f0>,
 			      <0 0x08000000 0 0x04000000>,
 			      <0 0xee208000 0 0x100>;
